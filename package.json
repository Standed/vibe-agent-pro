{
  "name": "video-agent-pro",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@google/genai": "^1.30.0",
    "@google/generative-ai": "^0.21.0",
    "@supabase/auth-helpers-nextjs": "^0.14.0",
    "@supabase/supabase-js": "^2.87.1",
    "@types/file-saver": "^2.0.7",
    "@types/fluent-ffmpeg": "^2.1.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.0.8",
    "dexie-react-hooks": "^1.1.7",
    "file-saver": "^2.0.5",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^11.15.0",
    "immer": "^10.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.460.0",
    "next": "^15.1.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.5",
    "undici": "^7.16.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-serve": "^1.2.0",
    "electron-updater": "^6.1.7",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.0"
  },
  "main": "electron/dist/main.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:electron": "tsc -p electron/tsconfig.json && next build",
    "dist": "npm run build:electron && electron-builder"
  },
  "build": {
    "appId": "com.video.agent.pro",
    "productName": "Video Agent Pro",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "electron/dist/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "publish": {
      "provider": "github",
      "owner": "vibe-agent",
      "repo": "video-agent-pro"
    }
  }
}
